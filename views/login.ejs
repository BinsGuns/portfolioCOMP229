<div class="container text-center" >
    <div   class="card mx-auto" style="width: 18rem;">
        <img class="mx-auto" src="https://www.kadencewp.com/wp-content/uploads/2020/10/alogo-2.svg" alt="Logo" width="80" height="80">
        <div class="card-body">
          
            <form action="/login/password" method="post">
                <div class="form-floating mb-3">
<!--                    vince-->
                    <input required name="username"  type="text" class="form-control" id="floatingInput" placeholder="Username">
                    <label for="floatingInput">Username</label>
                </div>
                <div class="form-floating">
<!--                    123456-->
                    <input required name="password"  type="password" class="form-control" id="floatingPassword" placeholder="Password">
                    <label for="floatingPassword">Password</label>
                </div>
                <div class="d-grid gap-2 mt-4">
                <button class="btn btn-primary btn-block" type="submit">SIGN IN</button>
                </div>
            </form>
        </div>
    </div>
   
    
    
</div>